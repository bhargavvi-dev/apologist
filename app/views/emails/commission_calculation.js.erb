<% if (@commission and @commission.errors.any?) or @error.present? %>
  <% error_msg = @error.present? ? t('general.something_went_wrong') : @commission.errors.full_messages.first%>
  setFlashMessage("set_flash_msg", "set_flash_msg", "<%= t('general.something_went_wrong') %>");
<% else %>
  setFlashMessage("set_flash_msg", "set_flash_msg", "<%= t('message.email_successfully_sent') %>");
<% end %>
$('.modal').modal('hide');
setTimeout(function(){
  $('#commission_calculation_step_3').modal('show');
}, 1000);
<%= render :partial => "shared/ajax_js" %>