- if @users.present?
  - @users.each do |user|
    = render :partial => 'user', :locals => {user: user}
  - if @users.count > Settings.system.per_page
    - count = params[:page].to_i + 1
    = link_to next_users_users_path(count), :remote => true, :class => "loadMore jscroll-next" do
      %i.downArrow
      = t("general.next")